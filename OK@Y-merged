#!/usr/bin/env python3

import math




def dc(a, b):
    ''' dc = lambda a, b: math.cos(a) * b, math.tan(a) * b '''
    return math.cos(a) * b, math.tan(a) * b
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
